  "main": "index.js",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node .build/src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon -r tsconfig-paths/register src/index.ts",
    "build": "tsc --project tsconfig.build.json && tsc-alias",
    "lint": "eslint --ignore-path .gitignore --ext .ts src",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --watchAll --no-cache",
    "test:ci": "jest"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.7.2",
    "express": "^4.19.2",
    "tsc-alias": "^1.8.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.8",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2",
    "typescript-transform-paths": "^3.4.7"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
